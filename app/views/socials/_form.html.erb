<div class="contact-form">
	  <div class="title"><h3>social_icon</h3></div>
	  <%# binding.pry %>
	    <%= form_for social do |form|%>
	    	<div class="row clearfix">
	    		<div class="form-group col-md-4 col-sm-6 col-xs-12">
	    			<%= form.label :facebook_links %>
	    			<%= form.text_field :facebook_links , placeholder: "Enter Your facebook_link" %>
	    		</div>
	    		<div class="form-group col-md-4 col-sm-6 col-xs-12">
	    			<%= form.label :twitter_links %>
	    			<%= form.text_field :twitter_links , placeholder: "Enter Your twitter_links" %>
	    		</div>
	    		<div class="form-group col-md-4 col-sm-6 col-xs-12">
	    			<%= form.label :google_links %>
	    			<%= form.text_field :google_links , placeholder: "Enter Your google_links" %>
	    		</div>
	    		<div class="form-group col-md-4 col-sm-6 col-xs-12">
	    			<%= form.label :pinterest_links %>
	    			<%= form.text_field :pinterest_links , placeholder: "Enter Your pinterest_links" %>
	    		</div>
	    		<div class="form-group col-md-4 col-sm-6 col-xs-12">
	    			<%= form.label :dribbble_links %>
	    			<%= form.text_field :dribbble_links , placeholder: "Enter Your dribbble_links" %>
	    		</div>
	    		
	    		<div class="form-group col-md-12 col-sm-12 col-xs-12">
	    			<%= form.submit class: "btn btn-danger" %>

	    		</div>

	    	</div>

</div>
<% end %>

